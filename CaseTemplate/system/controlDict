FoamFile
{
    version             2.0;
    format              ascii;
    class               dictionary;
    object              controlDict;
}

application             foamRun;
solver                  incompressibleFluid;
startFrom               latestTime;
startTime               0;
endTime                 5;
deltaT                  0.005;
writeControl            timeStep;
writeInterval           500000; //주석이지만 작동에 꼭 필요한 주석이니 지우지 마세요
purgeWrite              0;
writeFormat             ascii;
writePrecision          6;
writeCompression        off;
runtimeModifiable       yes;
functions
{
    Forces
    {
        type            forces;
        patches         ("human");
        rho             rhoInf;
        rhoInf          1.225;
        LiftDir         (0 0 1);
        DragDir         (0 1 0);
        CofR            (0 0 0);
        writeControl    timeStep;
        writeInterval   1;
        timeAverage     yes;
        timeStart       0;
        timeEnd         5;
    }
}

